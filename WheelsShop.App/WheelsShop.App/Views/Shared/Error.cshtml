@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<h1 class="text-danger">Error.</h1>
<h2 class="text-danger">An error occurred while processing your request.</h2>

<h3>Action: @Model.ActionName</h3>
<h3>Controller: @Model.ControllerName</h3>
<h3>Message: @Model.Exception.Message</h3>

<h3>Message: @Model.Exception.HResult</h3>

<h3>Message: @Model.Exception</h3>
