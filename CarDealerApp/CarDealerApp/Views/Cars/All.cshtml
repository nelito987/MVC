@using CarDealer.Models.ViewModels
@model IEnumerable<CarVm>

@{
    ViewBag.Title = "All";
}

<h2>All cars</h2>

<a class="btn btn-primary" href="/cars/add">Add Car</a>

<table class="table table-striped">
    <tr>
        <th>Make</th>
        <th>Model</th>
        <th>Travelled Distance</th>
    </tr>
   @foreach (var car in Model)
   {
    <tr>
        <th>@car.Make</th>
        <th>@car.Model</th>
        <th>@car.TravelledDistance</th>
    </tr>
   }
</table>

