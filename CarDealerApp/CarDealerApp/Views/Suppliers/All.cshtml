@using CarDealer.Models.ViewModels
@model IEnumerable<SupplierVm>

@{
    ViewBag.Title = "All";
}

<h2>All suppliers by filter</h2>

<table class="table table-striped">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Number Of Parts</th>
    </tr>
    @foreach (var supplier in Model)
    {
        <tr>
            <th>@supplier.Id</th>
            <th>@supplier.Name</th>
            <th>@supplier.PartsCount</th>
        </tr>
    }
</table>


